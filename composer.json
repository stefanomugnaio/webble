{
    "name": "symfony/skeleton",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,

    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",

        "symfony/framework-bundle": "^7.2",
        "symfony/console": "^7.2",
        "symfony/dotenv": "^7.2",
        "symfony/runtime": "^7.2",
        "symfony/yaml": "^7.2",

        "symfony/twig-bundle": "^7.2",
        "twig/twig": "^3.0",

        "symfony/form": "^7.2",
        "symfony/validator": "^7.2",
        "symfony/security-bundle": "^7.2",

        "symfony/asset": "^7.2",
        "symfony/webpack-encore-bundle": "^2.4",

        "symfony/mailer": "^7.2",
        "symfony/monolog-bundle": "^3.0",

        "doctrine/doctrine-bundle": "^2.13",
        "doctrine/orm": "^2.17",
        "doctrine/doctrine-migrations-bundle": "^3.7"
    },

    "require-dev": {
        "symfony/maker-bundle": "^1.60",
        "symfony/debug-bundle": "^7.2",
        "symfony/web-profiler-bundle": "^7.2",
        "phpunit/phpunit": "^10.5"
    },

    "config": {
        "platform": {
            "php": "8.2.0"
        },
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },

    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },

    "scripts": {
        "auto-scripts": {
            "cache:clear": "php bin/console cache:clear",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },

    "conflict": {
        "symfony/symfony": "*"
    },

    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^7.2"
        }
    }
}
